{
  "name": "dott_server",
  "version": "1.0.0",
  "main": "index.js",
  "author": "stringsaeed",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production ts-node --transpile-only src/server.ts",
    "dev": "cross-env NODE_ENV=development nodemon --watch src --delay 1 --exec 'ts-node' src/index.ts",
    "generate:prisma": "prisma generate",
    "generate:nexus": "cross-env NODE_ENV=development ts-node --transpile-only src/graphql/schema.ts",
    "generate": "yarn generate:prisma && yarn run generate:nexus",
    "postinstall": "yarn generate"
  },
  "dependencies": {
    "@nexus/schema": "^0.14.0",
    "@prisma/client": "2.1.3",
    "apollo-server-express": "^2.15.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.2.0",
    "graphql-import": "^1.0.2",
    "helmet": "^3.23.3",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "nexus-plugin-jwt-auth": "^1.2.0",
    "nexus-plugin-prisma": "^0.16.1"
  },
  "devDependencies": {
    "@prisma/cli": "2.1.3",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/helmet": "^0.0.47",
    "@types/hpp": "^0.2.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.14",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.9.6"
  },
  "resolutions": {
    "graphql": "^15.2.0"
  }
}
